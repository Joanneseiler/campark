{
  "name": "campark",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seeds/places.seed.js",
    "build": "npm run css && npm run js",
    "css": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss/styles.scss -o public/css",
    "js": "browserify js/main.js -p esmify -o public/bundle.js -v",
    "watch": "npm run watch-css & npm run watch-js",
    "watch-css": "nodemon -e html,scss -x \"npm run css\"",
    "watch-js": "watchify js/main.js -p esmify -o public/bundle.js -v"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "autocompleter": "^6.1.3",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "cloudinary": "^1.26.2",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "hbs": "^4.1.2",
    "mongoose": "^5.13.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-sass": "^7.0.1",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "browserify": "^17.0.0",
    "esmify": "^2.1.1",
    "nodemon": "^2.0.9",
    "watchify": "^4.0.0"
  }
}
